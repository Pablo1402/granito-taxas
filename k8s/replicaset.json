{
    "apiVersion":"apps/v1",
    "kind":"ReplicaSet",
    "metadata":{
        "name":"api-taxas",
        "labels":{
            "app":"api-taxas",
            "version":"v1"
        }
    },
    "spec":{
        "replicas":1,
        "selector":{
            "matchLabels":{
                "version":"v1",
                "app":"api-taxas"
            }
        },
        "template":{
            "metadata":{
                "labels":{
                    "app":"api-taxas",
                    "version":"v1"
                }
            },
            "spec":{
                "containers":[
                    {
                        "name":"api-taxas",
                        "image":"pablo123/granito-taxa",
                        "ports":[
                            {
                                "containerPort":80
                            }
                        ],
                        "env":[
                            {
                                "name":"TAXA",
                                "value":"1"
                            }
                        ]
                    }
                ]
            }
        }
    }
}